          },
          "controls": {
            "mouseWheelZoom": true,
            "showZoom": true,
            "showAttribution": true
          },
          "basemap": {
            "type": "osm-standard"
          },
          "layers": [
            {
              "type": "markers",
              "config": {
                "size": {
                  "field": "Value",

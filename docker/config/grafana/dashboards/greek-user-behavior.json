                "vis": false
              }
            },
            "mappings": [
              {"options": {"athens": {"text": "ŒëŒ∏ŒÆŒΩŒ±"}}, "type": "value"},
              {"options": {"thessaloniki": {"text": "ŒòŒµœÉœÉŒ±ŒªŒøŒΩŒØŒ∫Œ∑"}}, "type": "value"},
              {"options": {"patras": {"text": "Œ†Œ¨œÑœÅŒ±"}}, "type": "value"},
              {"options": {"heraklion": {"text": "ŒóœÅŒ¨Œ∫ŒªŒµŒπŒø"}}, "type": "value"},
              {"options": {"larissa": {"text": "ŒõŒ¨œÅŒπœÉŒ±"}}, "type": "value"}
            ],
            "color": {"mode": "continuous-GrYlRd"}
          }
        },
        "options": {
          "view": {
            "id": "coords",
            "lat": 39.0742,
            "lon": 21.8243,

{
  "name": "dixis-fresh",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "dev:debug-brk": "NODE_OPTIONS='--inspect-brk' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:production": "NODE_ENV=production next build",
    "build:analyze": "ANALYZE=true npm run build",
    "analyze": "ANALYZE=true npm run build",
    "lighthouse": "lighthouse http://localhost:3001 --output=html --output-path=./lighthouse-report.html",
    "perf:audit": "npm run build && npm run lighthouse",
    "perf:analyze": "npm run build:analyze && npm run lighthouse",
    "start:production": "NODE_ENV=production next start",
    "preview": "npm run build && npm run start",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:install": "playwright install",
    "test:e2e:auth": "playwright test tests/e2e/auth",
    "test:e2e:products": "playwright test tests/e2e/products",
    "test:e2e:cart": "playwright test tests/e2e/cart",
    "test:e2e:dashboard": "playwright test tests/e2e/dashboard",
    "test:e2e:api": "playwright test tests/e2e/api",
    "test:e2e:visual": "playwright test tests/e2e/visual",
    "test:e2e:performance": "playwright test tests/e2e/performance",
    "test:e2e:accessibility": "playwright test tests/e2e/accessibility",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome'",
    "test:e2e:report": "playwright show-report",
    "clean": "rm -rf .next out dist",
    "clean:cache": "rm -rf .next/cache",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1",
    "validate-env": "node scripts/validate-env.js",
    "security-check": "npm audit --audit-level moderate",
    "deploy:staging": "npm run build:production && npm run validate-env && npm run start:production",
    "deploy:production": "npm run clean && npm run security-check && npm run type-check && npm run build:production && npm run validate-env",
    "deploy": "bash scripts/deploy.sh",
    "deploy:quick": "bash scripts/deploy.sh --skip-tests",
    "deploy:analyze": "bash scripts/deploy.sh --analyze",
    "production:start": "NODE_ENV=production npm start",
    "production:build": "NODE_ENV=production npm run build",
    "production:test": "NODE_ENV=production npm run validate-env && npm run type-check && npm run lint",
    "verify:all": "npm run type-check && npm run lint",
    "security:audit": "node scripts/security-audit.js",
    "pre-deploy": "npm run security:audit && npm run type-check && npm run lint",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "build:seo": "npm run generate:sitemap && npm run build"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.3.1",
    "@tanstack/react-query": "^5.77.2",
    "@tanstack/react-query-devtools": "^5.77.2",
    "autoprefixer": "^10.4.21",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.511.0",
    "next": "15.3.2",
    "postcss": "^8.5.6",
    "puppeteer": "^24.10.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.58.1",
    "react-hot-toast": "^2.5.2",
    "recharts": "^3.0.2",
    "sonner": "^2.0.5",
    "stripe": "^18.2.1",
    "tailwind-merge": "^3.3.0",
    "web-vitals": "^5.0.2",
    "zod": "^3.25.67",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.3.4",
    "@playwright/test": "^1.48.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "compression-webpack-plugin": "^11.0.0",
    "critters": "^0.0.23",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}

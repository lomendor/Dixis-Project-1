-export function parseApiError(error: unknown): ApiError {
-  if (error instanceof AxiosError) {
-    const status = error.response?.status || 0;
-    const data = error.response?.data;
-
-    const apiError = new Error(data?.message || error.message || 'Unknown error') as ApiError;
-
-    // Determine error type
-    let type: ApiError['type'] = 'unknown';
-    if (status === 401) type = 'auth';
-    else if (status === 403) type = 'permission';
-    else if (status === 404) type = 'notFound';
-    else if (status === 422) type = 'validation';
-    else if (status >= 500) type = 'server';
-    else if (status === 0) type = 'network';
-
-    apiError.status = status;
-    apiError.code = data?.code || `HTTP_${status}`;
-    apiError.type = type;
-    apiError.details = data?.details;
-    apiError.originalError = error;
-
-    return apiError;
-  }
-
-  // Handle non-axios errors
-  const apiError = new Error(error instanceof Error ? error.message : 'Unknown error') as ApiError;
-  apiError.status = 0;
-  apiError.code = 'CLIENT_ERROR';
-  apiError.type = 'unknown';
-  apiError.originalError = error instanceof Error ? error : undefined;
-
-  return apiError;
-}

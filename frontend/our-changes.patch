+name: 🚀 Dixis CI/CD Pipeline
+
+on:
+  push:
+    branches: [ main, develop ]
+  pull_request:
+    branches: [ main, develop ]
+
+env:
+  NODE_VERSION: '20'
+  PHP_VERSION: '8.2'
+
+jobs:
+  # Frontend Testing and Quality Gates
+  frontend-quality:
+    name: 🎨 Frontend Quality Gates
+    runs-on: ubuntu-latest
+    
+    steps:
+    - name: 📥 Checkout code
+      uses: actions/checkout@v4
+      
+    - name: 📦 Setup Node.js
+      uses: actions/setup-node@v4
+      with:
+        node-version: ${{ env.NODE_VERSION }}
+        cache: 'npm'
+        cache-dependency-path: 'dixis-fresh/package-lock.json'
+        
+    - name: 📚 Install dependencies
+      working-directory: ./dixis-fresh
+      run: npm ci
+      
+    - name: 🔍 TypeScript check
+      working-directory: ./dixis-fresh
+      run: npm run type-check
+      
+    - name: 🧹 Lint check
+      working-directory: ./dixis-fresh
+      run: npm run lint
+      
+    - name: 🏗️ Build application
+      working-directory: ./dixis-fresh
+      run: npm run build
+      
+    - name: 🧪 Run tests
+      working-directory: ./dixis-fresh
+      run: npm run test
+      
+    - name: 📊 Bundle analysis
+      working-directory: ./dixis-fresh
+      run: npm run analyze || true
+      
+    - name: 🔒 Security audit
+      working-directory: ./dixis-fresh
+      run: npm audit --audit-level moderate
+
+  # Backend Testing and Quality Gates
+  backend-quality:
+    name: 🛠️ Backend Quality Gates
+    runs-on: ubuntu-latest
+    
+    services:
+      mysql:
+        image: mysql:8.0
+        env:
+          MYSQL_ROOT_PASSWORD: password
+          MYSQL_DATABASE: dixis_test
+        ports:
+          - 3306:3306
+        options: --health-cmd="mysqladmin ping" --health-interval=10s --health-timeout=5s --health-retries=3
+    
+    steps:
+    - name: 📥 Checkout code
+      uses: actions/checkout@v4
+      
+    - name: 🐘 Setup PHP
+      uses: shivammathur/setup-php@v2
+      with:
+        php-version: ${{ env.PHP_VERSION }}
+        extensions: mbstring, dom, fileinfo, mysql, pdo_mysql
+        
+    - name: 📚 Install Composer dependencies
+      working-directory: ./backend
+      run: composer install --prefer-dist --no-progress --no-interaction
+      
+    - name: 📋 Copy environment file
+      working-directory: ./backend
+      run: cp .env.example .env
+      
+    - name: 🔑 Generate application key
+      working-directory: ./backend
+      run: php artisan key:generate
+      
+    - name: 🗄️ Run database migrations
+      working-directory: ./backend
+      env:
+        DB_CONNECTION: mysql
+        DB_HOST: 127.0.0.1
+        DB_PORT: 3306
+        DB_DATABASE: dixis_test
+        DB_USERNAME: root
+        DB_PASSWORD: password
+      run: php artisan migrate --force
+      
+    - name: 🧪 Run PHPUnit tests
+      working-directory: ./backend
+      env:
+        DB_CONNECTION: mysql
+        DB_HOST: 127.0.0.1
+        DB_PORT: 3306
+        DB_DATABASE: dixis_test
+        DB_USERNAME: root
+        DB_PASSWORD: password
+      run: vendor/bin/phpunit
+      
+    - name: 🔍 PHP Static Analysis
+      working-directory: ./backend
+      run: composer run-script phpstan || true
+
+  # Security Scan
+  security-scan:
+    name: 🔒 Security Scan
+    runs-on: ubuntu-latest
+    needs: [frontend-quality, backend-quality]
+    
+    steps:
+    - name: 📥 Checkout code
+      uses: actions/checkout@v4
+      
+    - name: 🔍 Run Trivy vulnerability scanner
+      uses: aquasecurity/trivy-action@master
+      with:
+        scan-type: 'fs'
+        scan-ref: '.'
+        format: 'sarif'
+        output: 'trivy-results.sarif'
+        
+    - name: 📊 Upload Trivy scan results
+      uses: github/codeql-action/upload-sarif@v2
+      if: always()
+      with:
+        sarif_file: 'trivy-results.sarif'
+
+  # Performance Testing
+  performance-test:
+    name: ⚡ Performance Testing
+    runs-on: ubuntu-latest
+    needs: [frontend-quality]
+    
+    steps:
+    - name: 📥 Checkout code
+      uses: actions/checkout@v4
+      
+    - name: 📦 Setup Node.js
+      uses: actions/setup-node@v4
+      with:
+        node-version: ${{ env.NODE_VERSION }}
+        
+    - name: 📚 Install dependencies
+      working-directory: ./dixis-fresh
+      run: npm ci
+      
+    - name: 🏗️ Build for production
+      working-directory: ./dixis-fresh
+      run: npm run build
+      
+    - name: 🚀 Start production server
+      working-directory: ./dixis-fresh
+      run: npm start &
+      
+    - name: ⏱️ Wait for server
+      run: sleep 10
+      
+    - name: 🔍 Lighthouse CI
+      uses: treosh/lighthouse-ci-action@v10
+      with:
+        configPath: './dixis-fresh/.lighthouserc.json'
+        uploadArtifacts: true
+        temporaryPublicStorage: true
+
+  # Integration Tests
+  integration-test:
+    name: 🔗 Integration Testing
+    runs-on: ubuntu-latest
+    needs: [frontend-quality, backend-quality]
+    
+    steps:
+    - name: 📥 Checkout code
+      uses: actions/checkout@v4
+      
+    - name: 🐳 Build Docker containers
+      run: docker-compose -f docker-compose.test.yml build
+      
+    - name: 🚀 Start test environment
+      run: docker-compose -f docker-compose.test.yml up -d
+      
+    - name: ⏱️ Wait for services
+      run: sleep 30
+      
+    - name: 🧪 Run integration tests
+      run: |
+        docker-compose -f docker-compose.test.yml exec -T frontend npm run test:e2e
+        
+    - name: 🛑 Stop test environment
+      run: docker-compose -f docker-compose.test.yml down
+
+  # Deployment to Staging
+  deploy-staging:
+    name: 🚢 Deploy to Staging
+    runs-on: ubuntu-latest
+    needs: [frontend-quality, backend-quality, security-scan, performance-test]
+    if: github.ref == 'refs/heads/develop' && github.event_name == 'push'
+    
+    environment:
+      name: staging
+      url: https://staging.dixis.io
+      
+    steps:
+    - name: 📥 Checkout code
+      uses: actions/checkout@v4
+      
+    - name: 📦 Setup Node.js
+      uses: actions/setup-node@v4
+      with:
+        node-version: ${{ env.NODE_VERSION }}
+        
+    - name: 🏗️ Build frontend
+      working-directory: ./dixis-fresh
+      run: |
+        npm ci
+        npm run build
+        
+    - name: 📦 Create deployment package
+      run: |
+        tar -czf dixis-staging-${{ github.sha }}.tar.gz \
+          --exclude='node_modules' \
+          --exclude='.git' \
+          dixis-fresh/ backend/
+          
+    - name: 🚀 Deploy to staging
+      run: |
+        echo "Deploying to staging environment..."
+        # Add staging deployment commands here
+
+  # Production Deployment
+  deploy-production:
+    name: 🌟 Deploy to Production
+    runs-on: ubuntu-latest
+    needs: [frontend-quality, backend-quality, security-scan, performance-test, integration-test]
+    if: (github.ref == 'refs/heads/main' || github.ref == 'refs/heads/recovery-440e871') && github.event_name == 'push'
+    
+    environment:
+      name: production
+      url: https://dixis.io
+      
+    steps:
+    - name: 📥 Checkout code
+      uses: actions/checkout@v4
+      
+    - name: 📦 Setup Node.js
+      uses: actions/setup-node@v4
+      with:
+        node-version: ${{ env.NODE_VERSION }}
+        
+    - name: 🏗️ Build frontend
+      working-directory: ./dixis-fresh
+      run: |
+        npm ci
+        npm run build
+        
+    - name: 📦 Create deployment package
+      run: |
+        tar -czf dixis-production-${{ github.sha }}.tar.gz \
+          --exclude='node_modules' \
+          --exclude='.git' \
+          dixis-fresh/ backend/
+          
+    - name: 🗄️ Setup PostgreSQL on VPS
+      run: |
+        echo "Setting up PostgreSQL on VPS..."
+        # SSH to VPS and install PostgreSQL
+        echo "${{ secrets.VPS_SSH_KEY }}" > vps_key
+        chmod 600 vps_key
+        
+        ssh -i vps_key -o StrictHostKeyChecking=no ${{ secrets.VPS_USER }}@${{ secrets.VPS_HOST }} << 'EOF'
+          # Install PostgreSQL if not exists
+          if ! command -v psql &> /dev/null; then
+            apt update
+            apt install -y postgresql postgresql-contrib
+            systemctl start postgresql
+            systemctl enable postgresql
+          fi
+          
+          # Create database and user
+          sudo -u postgres psql << 'PSQL'
+            CREATE DATABASE IF NOT EXISTS dixis_production;
+            CREATE USER IF NOT EXISTS postgres WITH PASSWORD 'dixis_secure_password_2025';
+            GRANT ALL PRIVILEGES ON DATABASE dixis_production TO postgres;
+            ALTER USER postgres CREATEDB;
+        PSQL
+          
+          echo "PostgreSQL setup completed"
+        EOF
+        
+        rm -f vps_key
+        
+    - name: 🚀 Deploy to production
+      run: |
+        echo "Deploying to production environment..."
+        # Use the professional-deploy.sh script
+        echo "${{ secrets.VPS_SSH_KEY }}" > vps_key
+        chmod 600 vps_key
+        
+        # Copy PostgreSQL environment configuration
+        scp -i vps_key backend/.env.production.postgresql ${{ secrets.VPS_USER }}@${{ secrets.VPS_HOST }}:/root/dixis-backend/.env.production
+        
+        # Run Laravel migrations on PostgreSQL
+        ssh -i vps_key ${{ secrets.VPS_USER }}@${{ secrets.VPS_HOST }} << 'EOF'
+          cd /root/dixis-backend
+          php artisan config:clear
+          php artisan migrate --force
+          php artisan db:seed --force
+          echo "Database migration completed"
+        EOF
+        
+        # Deploy using professional script
+        ./scripts/professional-deploy.sh --frontend-only
+        
+        rm -f vps_key
+        
+    - name: 🔍 Post-deployment verification
+      run: |
+        # Wait for deployment
+        sleep 30
+        # Verify site is responding
+        curl -f https://dixis.io || exit 1
+        # Verify API is responding
+        curl -f https://dixis.io/api/v1/health || exit 1
+        
+    - name: 📢 Notify success
+      if: success()
+      run: |
+        echo "🎉 Production deployment successful!"
+        echo "Site: https://dixis.io"
+        echo "Commit: ${{ github.sha }}"
+
+  # Notification
+  notify:
+    name: 📢 Notification
+    runs-on: ubuntu-latest
+    needs: [deploy-production]
+    if: always()
+    
+    steps:
+    - name: 📊 Status notification
+      run: |
+        if [ "${{ needs.deploy-production.result }}" == "success" ]; then
+          echo "✅ Pipeline completed successfully"
+        else
+          echo "❌ Pipeline failed - check logs"
+        fi

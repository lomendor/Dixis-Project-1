- */
-const createQueryClient = (networkStatus: { online: boolean }) => {
-  return new QueryClient({
-    defaultOptions: {
-      queries: {
-        // Network-aware refetch behavior
-        refetchOnWindowFocus: networkStatus.online,
-        refetchOnReconnect: networkStatus.online,
-        refetchOnMount: networkStatus.online,
-
-        // Optimize for offline usage
-        retry: networkStatus.online ? 2 : 0,
-
-        // Cache configuration
-        staleTime: isServer ? 10 * 60 * 1000 : 5 * 60 * 1000, // 10 mins server, 5 mins client
-        gcTime: 15 * 60 * 1000, // 15 minutes
-
-        // Suspense support
-        suspense: false,
-
-        // Show better error messages in development
-        useErrorBoundary: process.env.NODE_ENV === 'production',

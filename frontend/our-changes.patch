+<?php
+
+namespace App\Services\B2B;
+
+use App\Models\BusinessUser;
+use App\Models\B2B\CreditLimit;
+use App\Models\B2B\CreditTransaction;
+use Illuminate\Support\Facades\DB;
+use Illuminate\Support\Facades\Log;
+
+class CreditLimitService
+{
+    /**
+     * Get credit status for a business user
+     */
+    public function getCreditStatus(BusinessUser $businessUser): array
+    {
+        $creditLimit = $businessUser->creditLimit;
+        
+        if (!$creditLimit) {
+            return [
+                'limit' => 0,
+                'used' => 0,
+                'available' => 0,
+                'status' => 'no_credit'
+            ];
+        }
+
+        $usedCredit = $this->getUsedCredit($businessUser);
+        $availableCredit = max(0, $creditLimit->limit - $usedCredit);
+
+        return [
+            'limit' => $creditLimit->limit,
+            'used' => $usedCredit,
+            'available' => $availableCredit,
+            'status' => $creditLimit->status
+        ];
+    }
+
+    /**
+     * Get used credit amount for a business user
+     */
+    protected function getUsedCredit(BusinessUser $businessUser): float
+    {
+        return CreditTransaction::where('business_user_id', $businessUser->id)
+            ->where('type', 'debit')
+            ->where('status', 'pending')
+            ->sum('amount');
+    }
+
+    /**
+     * Check if business user has sufficient credit
+     */
+    public function hasSufficientCredit(BusinessUser $businessUser, float $amount): bool
+    {
+        $creditStatus = $this->getCreditStatus($businessUser);
+        return $creditStatus['available'] >= $amount;
+    }
+
+    /**
+     * Reserve credit for an order
+     */
+    public function reserveCredit(BusinessUser $businessUser, float $amount, string $reference = null): bool
+    {
+        if (!$this->hasSufficientCredit($businessUser, $amount)) {
+            return false;
+        }
+
+        try {
+            CreditTransaction::create([
+                'business_user_id' => $businessUser->id,
+                'type' => 'debit',
+                'amount' => $amount,
+                'reference' => $reference,
+                'status' => 'pending',
+                'description' => 'Credit reserved for order'
+            ]);
+
+            return true;
+        } catch (\Exception $e) {
+            Log::error('Failed to reserve credit', [
+                'business_user_id' => $businessUser->id,
+                'amount' => $amount,
+                'error' => $e->getMessage()
+            ]);
+            return false;
+        }
+    }
+
+    /**
+     * Confirm credit usage
+     */
+    public function confirmCredit(BusinessUser $businessUser, string $reference): bool
+    {
+        try {
+            CreditTransaction::where('business_user_id', $businessUser->id)
+                ->where('reference', $reference)
+                ->where('status', 'pending')
+                ->update(['status' => 'confirmed']);
+
+            return true;
+        } catch (\Exception $e) {
+            Log::error('Failed to confirm credit', [
+                'business_user_id' => $businessUser->id,
+                'reference' => $reference,
+                'error' => $e->getMessage()
+            ]);
+            return false;
+        }
+    }
+
+    /**
+     * Release reserved credit
+     */
+    public function releaseCredit(BusinessUser $businessUser, string $reference): bool
+    {
+        try {
+            CreditTransaction::where('business_user_id', $businessUser->id)
+                ->where('reference', $reference)
+                ->where('status', 'pending')
+                ->delete();
+
+            return true;
+        } catch (\Exception $e) {
+            Log::error('Failed to release credit', [
+                'business_user_id' => $businessUser->id,
+                'reference' => $reference,
+                'error' => $e->getMessage()
+            ]);
+            return false;
+        }
+    }
+
+    /**
+     * Get credit transaction history
+     */
+    public function getCreditTransactions(BusinessUser $businessUser, int $limit = 50): array
+    {
+        return CreditTransaction::where('business_user_id', $businessUser->id)
+            ->orderBy('created_at', 'desc')
+            ->limit($limit)
+            ->get()
+            ->toArray();
+    }
+
+    /**
+     * Request credit limit increase
+     */
+    public function requestCreditIncrease(BusinessUser $businessUser, float $requestedLimit, string $reason = null): bool
+    {
+        try {
+            // Logic to handle credit increase request
+            // This could involve creating a request record, sending notifications, etc.
+            
+            Log::info('Credit increase requested', [
+                'business_user_id' => $businessUser->id,
+                'requested_limit' => $requestedLimit,
+                'reason' => $reason
+            ]);
+
+            return true;
+        } catch (\Exception $e) {
+            Log::error('Failed to request credit increase', [
+                'business_user_id' => $businessUser->id,
+                'requested_limit' => $requestedLimit,
+                'error' => $e->getMessage()
+            ]);
+            return false;
+        }
+    }
+}

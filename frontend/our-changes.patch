-    queryKey: ProductQueryKeys.productsFiltered(filters),
-    queryFn: async ({ signal }) => {
-      try {
-        const response = await apiClient.get<ProductListResponse>(API_ENDPOINTS.PRODUCTS.BASE, {
-          params: filters,
-          signal

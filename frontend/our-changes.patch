+```bash
+cat > app/Services/B2B/CreditLimitService.php << 'EOFSERVICE'
+<?php
+
+namespace App\Services\B2B;
+
+use App\Models\BusinessUser;
+use App\Models\B2B\CreditLimit;
+use App\Models\B2B\CreditTransaction;
+use Illuminate\Support\Facades\DB;
+use Illuminate\Support\Facades\Log;
+
+class CreditLimitService
+{
+    public function getCreditStatus(BusinessUser $businessUser): array
+    {
+        $creditLimit = $businessUser->creditLimit;
+        
+        if (!$creditLimit) {
+            return [
+                'limit' => 0,
+                'used' => 0,
+                'available' => 0,
+                'status' => 'no_credit'
+            ];
+        }
+
+        $usedCredit = $this->getUsedCredit($businessUser);
+        $availableCredit = max(0, $creditLimit->limit - $usedCredit);
+
+        return [
+            'limit' => $creditLimit->limit,
+            'used' => $usedCredit,
+            'available' => $availableCredit,
+            'status' => $creditLimit->status
+        ];
+    }
+
+    protected function getUsedCredit(BusinessUser $businessUser): float
+    {
+        return CreditTransaction::where('business_user_id', $businessUser->id)
+            ->where('type', 'debit')
+            ->where('status', 'pending')
+            ->sum('amount');
+    }
+
+    public function hasSufficientCredit(BusinessUser $businessUser, float $amount): bool
+    {
+        $creditStatus = $this->getCreditStatus($businessUser);
+        return $creditStatus['available'] >= $amount;
+    }
+}

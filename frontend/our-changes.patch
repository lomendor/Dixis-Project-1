+#!/bin/bash
+
+# 🚀 DIXIS PROFESSIONAL DEPLOYMENT SCRIPT
+# Professional-grade deployment system for safe VPS updates
+# Supports logo changes, design updates, and feature deployments
+
+set -e  # Exit on any error
+
+# =============================================================================
+# CONFIGURATION
+# =============================================================================
+
+VPS_HOST="147.93.126.235"
+VPS_USER="root"
+SSH_KEY="$HOME/.ssh/dixis_new_key"
+VPS_FRONTEND_PATH="/var/www/dixis-frontend"
+VPS_BACKEND_PATH="/root/dixis-backend"
+LOCAL_FRONTEND_PATH="./dixis-fresh"
+LOCAL_BACKEND_PATH="./backend"
+
+# Color codes for output
+RED='\033[0;31m'
+GREEN='\033[0;32m'
+YELLOW='\033[1;33m'
+BLUE='\033[0;34m'
+PURPLE='\033[0;35m'
+NC='\033[0m' # No Color
+
+# =============================================================================
+# HELPER FUNCTIONS
+# =============================================================================
+
+log_info() {
+    echo -e "${BLUE}[INFO]${NC} $1"
+}
+
+log_success() {
+    echo -e "${GREEN}[SUCCESS]${NC} $1"
+}
+
+log_warning() {
+    echo -e "${YELLOW}[WARNING]${NC} $1"
+}
+
+log_error() {
+    echo -e "${RED}[ERROR]${NC} $1"
+}
+
+log_step() {
+    echo -e "${PURPLE}[STEP]${NC} $1"
+}
+
+# Check if required files exist
+check_prerequisites() {
+    log_step "Checking prerequisites..."
+    
+    # Check SSH key
+    if [ ! -f "$SSH_KEY" ]; then
+        log_error "SSH key not found at $SSH_KEY"
+        exit 1
+    fi
+    
+    # Check local frontend directory
+    if [ ! -d "$LOCAL_FRONTEND_PATH" ]; then
+        log_error "Local frontend directory not found at $LOCAL_FRONTEND_PATH"
+        exit 1
+    fi
+    
+    # Check if we can connect to VPS
+    if ! ssh -i "$SSH_KEY" -o ConnectTimeout=10 "$VPS_USER@$VPS_HOST" "echo 'Connection test'" > /dev/null 2>&1; then
+        log_error "Cannot connect to VPS. Check SSH key and network connection."
+        exit 1
+    fi
+    
+    log_success "Prerequisites check passed"
+}
+
+# Create backup on VPS
+create_backup() {
+    log_step "Creating VPS backup..."
+    
+    BACKUP_TIMESTAMP=$(date +%Y%m%d_%H%M%S)
+    BACKUP_DIR="/root/backups/dixis_backup_$BACKUP_TIMESTAMP"
+    
+    ssh -i "$SSH_KEY" "$VPS_USER@$VPS_HOST" << EOF
+        mkdir -p /root/backups
+        mkdir -p $BACKUP_DIR
+        
+        # Backup frontend
+        if [ -d "$VPS_FRONTEND_PATH" ]; then
+            cp -r $VPS_FRONTEND_PATH $BACKUP_DIR/frontend
+            echo "Frontend backed up to $BACKUP_DIR/frontend"
+        fi
+        
+        # Backup backend (only critical files)
+        if [ -d "$VPS_BACKEND_PATH" ]; then
+            mkdir -p $BACKUP_DIR/backend
+            cp $VPS_BACKEND_PATH/.env* $BACKUP_DIR/backend/ 2>/dev/null || true
+            cp -r $VPS_BACKEND_PATH/database $BACKUP_DIR/backend/ 2>/dev/null || true
+            echo "Backend configuration backed up to $BACKUP_DIR/backend"
+        fi
+        
+        echo "Backup created at: $BACKUP_DIR"
+EOF
+    
+    echo "$BACKUP_DIR" > .last_backup_path
+    log_success "Backup created: $BACKUP_DIR"
+}
+
+# Build frontend locally
+build_frontend() {
+    log_step "Building frontend locally..."
+    
+    cd "$LOCAL_FRONTEND_PATH"
+    
+    # Install dependencies if node_modules doesn't exist
+    if [ ! -d "node_modules" ]; then
+        log_info "Installing dependencies..."
+        npm install
+    fi
+    
+    # Build the application
+    log_info "Building Next.js application..."
+    npm run build
+    
+    # Check if build was successful
+    if [ ! -d ".next" ]; then
+        log_error "Build failed - .next directory not found"
+        exit 1
+    fi
+    
+    cd - > /dev/null
+    log_success "Frontend build completed"
+}
+
+# Deploy frontend to VPS
+deploy_frontend() {
+    log_step "Deploying frontend to VPS..."
+    
+    # Create deployment package
+    DEPLOY_PACKAGE="/tmp/dixis-frontend-$(date +%Y%m%d_%H%M%S).tar.gz"
+    
+    cd "$LOCAL_FRONTEND_PATH"
+    tar -czf "$DEPLOY_PACKAGE" \
+        --exclude='node_modules' \
+        --exclude='.git' \
+        --exclude='*.log' \
+        --exclude='.DS_Store' \
+        .
+    cd - > /dev/null
+    
+    log_info "Created deployment package: $DEPLOY_PACKAGE"
+    
+    # Upload to VPS
+    log_info "Uploading to VPS..."
+    scp -i "$SSH_KEY" "$DEPLOY_PACKAGE" "$VPS_USER@$VPS_HOST:/tmp/"
+    
+    # Extract and deploy on VPS
+    ssh -i "$SSH_KEY" "$VPS_USER@$VPS_HOST" << EOF
+        # Stop the service
+        pm2 stop dixis-frontend 2>/dev/null || true
+        
+        # Create new directory
+        rm -rf $VPS_FRONTEND_PATH.new
+        mkdir -p $VPS_FRONTEND_PATH.new
+        
+        # Extract new version
+        cd $VPS_FRONTEND_PATH.new
+        tar -xzf /tmp/$(basename $DEPLOY_PACKAGE)
+        
+        # Install dependencies
+        npm install --production
+        
+        # Build on VPS (ensures compatibility)
+        NODE_ENV=production npm run build
+        
+        # Copy environment file from current installation
+        if [ -f "$VPS_FRONTEND_PATH/.env.production" ]; then
+            cp $VPS_FRONTEND_PATH/.env.production .env.production
+        fi
+        
+        # Atomic switch
+        if [ -d "$VPS_FRONTEND_PATH" ]; then
+            mv $VPS_FRONTEND_PATH $VPS_FRONTEND_PATH.old
+        fi
+        mv $VPS_FRONTEND_PATH.new $VPS_FRONTEND_PATH
+        
+        # Start the service
+        cd $VPS_FRONTEND_PATH
+        NODE_ENV=production nohup node node_modules/.bin/next start > /dev/null 2>&1 &
+        
+        # Cleanup
+        rm -f /tmp/$(basename $DEPLOY_PACKAGE)
+        rm -rf $VPS_FRONTEND_PATH.old
+        
+        echo "Frontend deployment completed"
+EOF
+    
+    # Cleanup local deployment package
+    rm -f "$DEPLOY_PACKAGE"
+    
+    log_success "Frontend deployed successfully"
+}
+
+# Deploy backend changes (if any)
+deploy_backend() {
+    if [ "$DEPLOY_BACKEND" = "true" ]; then
+        log_step "Deploying backend changes..."
+        
+        # Create backend deployment package
+        BACKEND_PACKAGE="/tmp/dixis-backend-$(date +%Y%m%d_%H%M%S).tar.gz"
+        
+        cd "$LOCAL_BACKEND_PATH"
+        tar -czf "$BACKEND_PACKAGE" \
+            --exclude='vendor' \
+            --exclude='node_modules' \
+            --exclude='.git' \
+            --exclude='storage/logs/*' \
+            --exclude='*.log' \
+            .
+        cd - > /dev/null
+        
+        # Upload and deploy
+        scp -i "$SSH_KEY" "$BACKEND_PACKAGE" "$VPS_USER@$VPS_HOST:/tmp/"
+        
+        ssh -i "$SSH_KEY" "$VPS_USER@$VPS_HOST" << EOF
+            cd /tmp
+            tar -xzf $(basename $BACKEND_PACKAGE)
+            
+            # Copy specific files (be careful not to overwrite database)
+            cp -r app $VPS_BACKEND_PATH/
+            cp -r routes $VPS_BACKEND_PATH/
+            cp -r config $VPS_BACKEND_PATH/
+            cp composer.json $VPS_BACKEND_PATH/
+            
+            cd $VPS_BACKEND_PATH
+            composer install --no-dev --optimize-autoloader
+            php artisan config:cache
+            php artisan route:cache
+            php artisan view:cache
+            
+            # Restart backend service
+            php artisan queue:restart
+            
+            rm -f /tmp/$(basename $BACKEND_PACKAGE)
+            echo "Backend deployment completed"
+EOF
+        
+        rm -f "$BACKEND_PACKAGE"
+        log_success "Backend deployed successfully"
+    else
+        log_info "Skipping backend deployment (not requested)"
+    fi
+}
+
+# Verify deployment
+verify_deployment() {
+    log_step "Verifying deployment..."
+    
+    # Wait a moment for services to start
+    sleep 5
+    
+    # Check if site is responding
+    HTTP_STATUS=$(curl -s -o /dev/null -w "%{http_code}" "http://$VPS_HOST:3000" || echo "000")
+    
+    if [ "$HTTP_STATUS" = "200" ]; then
+        log_success "Frontend is responding (HTTP $HTTP_STATUS)"
+    else
+        log_warning "Frontend may have issues (HTTP $HTTP_STATUS)"
+    fi
+    
+    # Check backend
+    BACKEND_STATUS=$(curl -s -o /dev/null -w "%{http_code}" "http://$VPS_HOST:8000/api/v1/products" || echo "000")
+    
+    if [ "$BACKEND_STATUS" = "200" ]; then
+        log_success "Backend API is responding (HTTP $BACKEND_STATUS)"
+    else
+        log_warning "Backend API may have issues (HTTP $BACKEND_STATUS)"
+    fi
+    
+    # Test actual site
+    SITE_STATUS=$(curl -s -o /dev/null -w "%{http_code}" "https://dixis.io" || echo "000")
+    
+    if [ "$SITE_STATUS" = "200" ]; then
+        log_success "Production site is accessible (HTTP $SITE_STATUS)"
+    else
+        log_warning "Production site may have issues (HTTP $SITE_STATUS)"
+    fi
+}
+
+# Rollback function
+rollback() {
+    log_step "Rolling back to previous version..."
+    
+    if [ ! -f ".last_backup_path" ]; then
+        log_error "No backup path found. Cannot rollback."
+        exit 1
+    fi
+    
+    BACKUP_DIR=$(cat .last_backup_path)
+    
+    ssh -i "$SSH_KEY" "$VPS_USER@$VPS_HOST" << EOF
+        if [ -d "$BACKUP_DIR/frontend" ]; then
+            pm2 stop dixis-frontend 2>/dev/null || true
+            rm -rf $VPS_FRONTEND_PATH
+            cp -r $BACKUP_DIR/frontend $VPS_FRONTEND_PATH
+            cd $VPS_FRONTEND_PATH
+            NODE_ENV=production nohup node node_modules/.bin/next start > /dev/null 2>&1 &
+            echo "Frontend rolled back successfully"
+        fi
+        
+        if [ -d "$BACKUP_DIR/backend" ]; then
+            cp -r $BACKUP_DIR/backend/* $VPS_BACKEND_PATH/
+            cd $VPS_BACKEND_PATH
+            php artisan config:cache
+            echo "Backend rolled back successfully"
+        fi
+EOF
+    
+    log_success "Rollback completed"
+}
+
+# =============================================================================
+# MAIN DEPLOYMENT LOGIC
+# =============================================================================
+
+show_usage() {
+    echo "Usage: $0 [OPTIONS]"
+    echo ""
+    echo "Options:"
+    echo "  --frontend-only    Deploy only frontend changes"
+    echo "  --backend-only     Deploy only backend changes"
+    echo "  --full             Deploy both frontend and backend"
+    echo "  --rollback         Rollback to previous version"
+    echo "  --no-backup        Skip backup creation (dangerous)"
+    echo "  --no-build         Skip local build (use existing)"
+    echo "  --help             Show this help message"
+    echo ""
+    echo "Examples:"
+    echo "  $0 --frontend-only    # Deploy logo changes, design updates"
+    echo "  $0 --full             # Deploy complete changes"
+    echo "  $0 --rollback         # Emergency rollback"
+}
+
+# Parse command line arguments
+DEPLOY_FRONTEND="false"
+DEPLOY_BACKEND="false"
+CREATE_BACKUP="true"
+BUILD_LOCALLY="true"
+ROLLBACK_MODE="false"
+
+while [[ $# -gt 0 ]]; do
+    case $1 in
+        --frontend-only)
+            DEPLOY_FRONTEND="true"
+            shift
+            ;;
+        --backend-only)
+            DEPLOY_BACKEND="true"
+            shift
+            ;;
+        --full)
+            DEPLOY_FRONTEND="true"
+            DEPLOY_BACKEND="true"
+            shift
+            ;;
+        --rollback)
+            ROLLBACK_MODE="true"
+            shift
+            ;;
+        --no-backup)
+            CREATE_BACKUP="false"
+            shift
+            ;;
+        --no-build)
+            BUILD_LOCALLY="false"
+            shift
+            ;;
+        --help)
+            show_usage
+            exit 0
+            ;;
+        *)
+            log_error "Unknown option: $1"
+            show_usage
+            exit 1
+            ;;
+    esac
+done
+
+# Default to frontend-only if no specific deployment type specified
+if [ "$DEPLOY_FRONTEND" = "false" ] && [ "$DEPLOY_BACKEND" = "false" ] && [ "$ROLLBACK_MODE" = "false" ]; then
+    DEPLOY_FRONTEND="true"
+fi
+
+# =============================================================================
+# MAIN EXECUTION
+# =============================================================================
+
+echo "🚀 DIXIS PROFESSIONAL DEPLOYMENT SYSTEM"
+echo "========================================"
+echo ""
+
+if [ "$ROLLBACK_MODE" = "true" ]; then
+    rollback
+    exit 0
+fi
+
+log_info "Deployment Configuration:"
+log_info "  Frontend: $DEPLOY_FRONTEND"
+log_info "  Backend: $DEPLOY_BACKEND"
+log_info "  Create Backup: $CREATE_BACKUP"
+log_info "  Build Locally: $BUILD_LOCALLY"
+echo ""
+
+# Execute deployment steps
+check_prerequisites
+
+if [ "$CREATE_BACKUP" = "true" ]; then
+    create_backup
+fi
+
+if [ "$DEPLOY_FRONTEND" = "true" ]; then
+    if [ "$BUILD_LOCALLY" = "true" ]; then
+        build_frontend
+    fi
+    deploy_frontend
+fi
+
+if [ "$DEPLOY_BACKEND" = "true" ]; then
+    deploy_backend
+fi
+
+verify_deployment
+
+echo ""
+log_success "🎉 DEPLOYMENT COMPLETED SUCCESSFULLY!"
+log_info "Your changes are now live on https://dixis.io"
+echo ""
+log_info "Useful commands:"
+log_info "  Rollback: $0 --rollback"
+log_info "  Re-deploy: $0 --frontend-only"
+echo ""

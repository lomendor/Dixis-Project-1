+#!/bin/bash
+
+# 🔍 DIXIS DEPLOYMENT VERIFICATION SCRIPT
+# Tests all critical functionality after deployment
+
+VPS_IP="147.93.126.235"
+FRONTEND_URL="http://$VPS_IP:3000"
+BACKEND_URL="http://$VPS_IP:8000/api/v1"
+
+echo "🔍 DIXIS DEPLOYMENT VERIFICATION"
+echo "================================"
+echo ""
+
+# Test 1: Backend API Health
+echo "🔄 Test 1: Backend API Health..."
+if curl -s -f "$BACKEND_URL/health" > /dev/null; then
+    echo "✅ Backend API is responding"
+else
+    echo "❌ Backend API is not responding"
+    exit 1
+fi
+
+# Test 2: Frontend Homepage
+echo "🔄 Test 2: Frontend Homepage..."
+if curl -s -f "$FRONTEND_URL" > /dev/null; then
+    echo "✅ Frontend homepage is responding"
+else
+    echo "❌ Frontend homepage is not responding"
+    exit 1
+fi
+
+# Test 3: Products API
+echo "🔄 Test 3: Products API..."
+PRODUCTS_COUNT=$(curl -s "$BACKEND_URL/products" | jq '.data | length' 2>/dev/null || echo "0")
+if [ "$PRODUCTS_COUNT" -gt 0 ]; then
+    echo "✅ Products API working ($PRODUCTS_COUNT products)"
+else
+    echo "❌ Products API not working"
+    exit 1
+fi
+
+# Test 4: Producer Dashboard
+echo "🔄 Test 4: Producer Dashboard..."
+if curl -s -f "$FRONTEND_URL/producer/dashboard" > /dev/null; then
+    echo "✅ Producer dashboard accessible"
+else
+    echo "❌ Producer dashboard not accessible"
+    exit 1
+fi
+
+# Test 5: B2B Portal
+echo "🔄 Test 5: B2B Portal..."
+if curl -s -f "$FRONTEND_URL/b2b/dashboard" > /dev/null; then
+    echo "✅ B2B portal accessible"
+else
+    echo "❌ B2B portal not accessible"
+    exit 1
+fi
+
+# Test 6: Shopping Cart
+echo "🔄 Test 6: Shopping Cart..."
+if curl -s -f "$FRONTEND_URL/cart" > /dev/null; then
+    echo "✅ Shopping cart accessible"
+else
+    echo "❌ Shopping cart not accessible"
+    exit 1
+fi
+
+# Test 7: Checkout Process
+echo "🔄 Test 7: Checkout Process..."
+if curl -s -f "$FRONTEND_URL/checkout" > /dev/null; then
+    echo "✅ Checkout process accessible"
+else
+    echo "❌ Checkout process not accessible"
+    exit 1
+fi
+
+# Test 8: Search Functionality
+echo "🔄 Test 8: Search Functionality..."
+if curl -s -f "$FRONTEND_URL/search" > /dev/null; then
+    echo "✅ Search functionality accessible"
+else
+    echo "❌ Search functionality not accessible"
+    exit 1
+fi
+
+echo ""
+echo "🎉 ALL TESTS PASSED!"
+echo "✅ DIXIS MARKETPLACE IS FULLY OPERATIONAL"
+echo ""
+echo "📊 DEPLOYMENT SUMMARY:"
+echo "- Frontend: $FRONTEND_URL"
+echo "- Backend: $BACKEND_URL"
+echo "- Products: $PRODUCTS_COUNT available"
+echo "- Status: Ready for customer usage"
+echo ""
+echo "🚀 READY FOR REVENUE GENERATION!"

+<?php
+
+namespace App\Http\Controllers\Api\Admin;
+
+use App\Http\Controllers\Controller;
+use App\Models\BusinessUser;
+use App\Models\User;
+use App\Services\B2B\CreditLimitService;
+use Illuminate\Http\JsonResponse;
+use Illuminate\Http\Request;
+use Illuminate\Support\Facades\Validator;
+
+class BusinessUserController extends Controller
+{
+    protected CreditLimitService $creditLimitService;
+
+    public function __construct(CreditLimitService $creditLimitService)
+    {
+        $this->creditLimitService = $creditLimitService;
+    }
+
+    /**
+     * Get all business users with pagination and filters
+     */
+    public function index(Request $request): JsonResponse
+    {
+        $query = BusinessUser::with(['user', 'creditLimit'])
+            ->orderBy('created_at', 'desc');
+
+        // Apply filters
+        if ($request->has('status')) {
+            $query->where('status', $request->status);
+        }
+
+        if ($request->has('verification_status')) {
+            $query->where('verification_status', $request->verification_status);
+        }
+
+        if ($request->has('search')) {
+            $search = $request->search;
+            $query->whereHas('user', function ($q) use ($search) {
+                $q->where('name', 'like', "%{$search}%")
+                  ->orWhere('email', 'like', "%{$search}%");
+            })->orWhere('company_name', 'like', "%{$search}%")
+              ->orWhere('vat_number', 'like', "%{$search}%");
+        }
+
+        if ($request->has('company_size')) {
+            $query->where('company_size', $request->company_size);
+        }
+
+        if ($request->has('industry')) {
+            $query->where('industry', $request->industry);
+        }
+
+        $businessUsers = $query->paginate($request->get('per_page', 15));
+
+        // Add credit status to each business user
+        $businessUsers->getCollection()->transform(function ($businessUser) {
+            $businessUser->credit_status = $this->creditLimitService->getCreditStatus($businessUser);
+            return $businessUser;
+        });
+
+        return response()->json($businessUsers);
+    }
+
+    /**
+     * Get a specific business user with detailed information
+     */
+    public function show(int $id): JsonResponse
+    {
+        $businessUser = BusinessUser::with([
+            'user',
+            'creditLimit',
+            'creditTransactions' => function ($query) {
+                $query->orderBy('created_at', 'desc')->limit(10);
+            },
+            'orders' => function ($query) {
+                $query->orderBy('created_at', 'desc')->limit(5);
+            }
+        ])->findOrFail($id);
+
+        // Add credit status and statistics
+        $businessUser->credit_status = $this->creditLimitService->getCreditStatus($businessUser);
+        $businessUser->credit_transactions_history = $this->creditLimitService->getCreditTransactions($businessUser, 20);
+
+        return response()->json($businessUser);
+    }
+
+    /**
+     * Update business user information
+     */
+    public function update(Request $request, int $id): JsonResponse
+    {
+        $validator = Validator::make($request->all(), [
+            'status' => 'sometimes|in:active,inactive,suspended',
+            'verification_status' => 'sometimes|in:pending,verified,rejected',
+            'credit_limit' => 'sometimes|numeric|min:0|max:100000',
+            'payment_terms' => 'sometimes|integer|min:1|max:90',
+            'admin_notes' => 'sometimes|string|max:2000'
+        ]);
+
+        if ($validator->fails()) {
+            return response()->json(['errors' => $validator->errors()], 422);
+        }
+
+        $businessUser = BusinessUser::findOrFail($id);
+
+        // Update business user fields
+        if ($request->has('status')) {
+            $businessUser->status = $request->status;
+        }
+
+        if ($request->has('verification_status')) {
+            $businessUser->verification_status = $request->verification_status;
+        }
+
+        if ($request->has('payment_terms')) {
+            $businessUser->payment_terms = $request->payment_terms;
+        }
+
+        if ($request->has('admin_notes')) {
+            $businessUser->admin_notes = $request->admin_notes;
+        }
+
+        $businessUser->save();
+
+        // Update credit limit if provided
+        if ($request->has('credit_limit')) {
+            $this->updateCreditLimit($businessUser, $request->credit_limit);
+        }
+
+        return response()->json([
+            'message' => 'Business user updated successfully',
+            'business_user' => $businessUser->load('user', 'creditLimit')
+        ]);
+    }
+
+    /**
+     * Get business user statistics for admin dashboard
+     */
+    public function getStats(): JsonResponse
+    {
+        $stats = [
+            'total_business_users' => BusinessUser::count(),
+            'active_business_users' => BusinessUser::where('status', 'active')->count(),
+            'pending_verification' => BusinessUser::where('verification_status', 'pending')->count(),
+            'verified_business_users' => BusinessUser::where('verification_status', 'verified')->count(),
+            'total_credit_extended' => BusinessUser::whereHas('creditLimit')
+                ->join('credit_limits', 'business_users.id', '=', 'credit_limits.business_user_id')
+                ->sum('credit_limits.limit'),
+            'monthly_registrations' => BusinessUser::whereMonth('created_at', now()->month)
+                ->whereYear('created_at', now()->year)
+                ->count(),
+            'credit_utilization' => $this->getCreditUtilizationStats()
+        ];
+
+        return response()->json($stats);
+    }
+
+    /**
+     * Verify a business user
+     */
+    public function verify(Request $request, int $id): JsonResponse
+    {
+        $validator = Validator::make($request->all(), [
+            'credit_limit' => 'sometimes|numeric|min:0|max:100000',
+            'payment_terms' => 'sometimes|integer|min:1|max:90',
+            'verification_notes' => 'sometimes|string|max:1000'
+        ]);
+
+        if ($validator->fails()) {
+            return response()->json(['errors' => $validator->errors()], 422);
+        }
+
+        $businessUser = BusinessUser::findOrFail($id);
+        
+        $businessUser->verification_status = 'verified';
+        $businessUser->status = 'active';
+        $businessUser->verified_at = now();
+        
+        if ($request->has('verification_notes')) {
+            $businessUser->admin_notes = $request->verification_notes;
+        }
+
+        if ($request->has('payment_terms')) {
+            $businessUser->payment_terms = $request->payment_terms;
+        }
+
+        $businessUser->save();
+
+        // Set initial credit limit if provided
+        if ($request->has('credit_limit') && $request->credit_limit > 0) {
+            $this->updateCreditLimit($businessUser, $request->credit_limit);
+        }
+
+        return response()->json([
+            'message' => 'Business user verified successfully',
+            'business_user' => $businessUser->load('user', 'creditLimit')
+        ]);
+    }
+
+    /**
+     * Reject a business user application
+     */
+    public function reject(Request $request, int $id): JsonResponse
+    {
+        $validator = Validator::make($request->all(), [
+            'rejection_reason' => 'required|string|max:1000'
+        ]);
+
+        if ($validator->fails()) {
+            return response()->json(['errors' => $validator->errors()], 422);
+        }
+
+        $businessUser = BusinessUser::findOrFail($id);
+        
+        $businessUser->verification_status = 'rejected';
+        $businessUser->status = 'inactive';
+        $businessUser->admin_notes = $request->rejection_reason;
+        
+        $businessUser->save();
+
+        return response()->json([
+            'message' => 'Business user application rejected',
+            'business_user' => $businessUser->load('user')
+        ]);
+    }
+
+    /**
+     * Update credit limit for a business user
+     */
+    protected function updateCreditLimit(BusinessUser $businessUser, float $creditLimit): void
+    {
+        $businessUser->creditLimit()->updateOrCreate(
+            ['business_user_id' => $businessUser->id],
+            [
+                'limit' => $creditLimit,
+                'status' => 'active',
+                'updated_at' => now()
+            ]
+        );
+    }
+
+    /**
+     * Get credit utilization statistics
+     */
+    protected function getCreditUtilizationStats(): array
+    {
+        $businessUsersWithCredit = BusinessUser::whereHas('creditLimit')->with('creditLimit')->get();
+        
+        $totalLimit = $businessUsersWithCredit->sum('creditLimit.limit');
+        $totalUsed = 0;
+        $highUtilization = 0;
+
+        foreach ($businessUsersWithCredit as $businessUser) {
+            $creditStatus = $this->creditLimitService->getCreditStatus($businessUser);
+            $totalUsed += $creditStatus['used'];
+            
+            if ($creditStatus['limit'] > 0) {
+                $utilizationPercentage = ($creditStatus['used'] / $creditStatus['limit']) * 100;
+                if ($utilizationPercentage > 80) {
+                    $highUtilization++;
+                }
+            }
+        }
+
+        return [
+            'total_limit' => $totalLimit,
+            'total_used' => $totalUsed,
+            'utilization_percentage' => $totalLimit > 0 ? ($totalUsed / $totalLimit) * 100 : 0,
+            'high_utilization_count' => $highUtilization
+        ];
+    }
+
+    /**
+     * Export business users data
+     */
+    public function export(Request $request): JsonResponse
+    {
+        $query = BusinessUser::with(['user', 'creditLimit']);
+
+        // Apply same filters as index method
+        if ($request->has('status')) {
+            $query->where('status', $request->status);
+        }
+
+        if ($request->has('verification_status')) {
+            $query->where('verification_status', $request->verification_status);
+        }
+
+        $businessUsers = $query->get();
+
+        // Format data for export
+        $exportData = $businessUsers->map(function ($businessUser) {
+            $creditStatus = $this->creditLimitService->getCreditStatus($businessUser);
+            
+            return [
+                'id' => $businessUser->id,
+                'company_name' => $businessUser->company_name,
+                'contact_name' => $businessUser->user->name,
+                'email' => $businessUser->user->email,
+                'vat_number' => $businessUser->vat_number,
+                'status' => $businessUser->status,
+                'verification_status' => $businessUser->verification_status,
+                'credit_limit' => $creditStatus['limit'],
+                'credit_used' => $creditStatus['used'],
+                'credit_available' => $creditStatus['available'],
+                'payment_terms' => $businessUser->payment_terms,
+                'created_at' => $businessUser->created_at->format('Y-m-d H:i:s'),
+                'verified_at' => $businessUser->verified_at?->format('Y-m-d H:i:s')
+            ];
+        });
+
+        return response()->json([
+            'data' => $exportData,
+            'total' => $exportData->count(),
+            'exported_at' => now()->format('Y-m-d H:i:s')
+        ]);
+    }
+}

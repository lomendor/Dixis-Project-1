-  private readonly MAX_ITEMS: number = 50; // Maximum number of items in cache
-
-  constructor() {
-    // Try to load cache from localStorage only in browser environment
-    if (typeof window !== 'undefined') {
-      try {
-        const savedCache = localStorage.getItem('api_cache');
-        if (savedCache) {
-          this.cache = JSON.parse(savedCache);
-          this.cleanExpiredItems(); // Clean expired items on load
-        }
-      } catch (error) {
-        logger.error('Error loading cache from localStorage:', { data: error, context: { feature: 'api-cache', action: 'load' } });
-        this.cache = {};
-      }
-    } else {
-      // Server-side rendering - use empty cache
-      this.cache = {};
-    }
-  }
-
-  // Generate a cache key from endpoint and params
-  private generateKey(endpoint: string, params?: any): string {
-    const paramsString = params ? JSON.stringify(params) : '';
-    return `${endpoint}:${paramsString}`;
-  }
-
-  // Get item from cache
-  get(endpoint: string, params?: any): any | null {
-    const key = this.generateKey(endpoint, params);
-    const item = this.cache[key];
-
-    // Check if item exists and is not expired
-    if (item && Date.now() - item.timestamp < this.TTL) {
-      logger.debug(`Cache hit for ${endpoint}`, { context: { feature: 'api-cache', action: 'get' } });
-      return item.data;
-    }
-
-    // Item doesn't exist or is expired
-    if (item) {
-      logger.debug(`Cache expired for ${endpoint}`, { context: { feature: 'api-cache', action: 'expire' } });
-      delete this.cache[key];
-      this.saveToLocalStorage();
-    }
-
-    return null;
-  }
-
-  // Set item in cache
-  set(endpoint: string, params: any, data: any): void {
-    // Clean cache if it's too large
-    if (Object.keys(this.cache).length >= this.MAX_ITEMS) {
-      this.cleanOldestItems();
-    }
-
-    const key = this.generateKey(endpoint, params);
-    this.cache[key] = {
-      data,
-      timestamp: Date.now(),
-      params: JSON.stringify(params)
-    };
-
-    logger.debug(`Cache set for ${endpoint}`, { context: { feature: 'api-cache', action: 'set' } });
-    this.saveToLocalStorage();
-  }
-
-  // Clean expired items
-  private cleanExpiredItems(): void {

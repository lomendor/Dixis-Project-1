-
-import React, { ReactNode, useState, useEffect } from 'react';
-import { QueryClient, QueryClientProvider } from '@tanstack/react-query';
-import { ReactQueryDevtools } from '@tanstack/react-query-devtools';
-import { NetworkStatusProvider } from './NetworkStatusProvider';
-import { ErrorProvider } from './ErrorProvider';
-import useNetworkStatus from '@/hooks/useNetworkStatus';
-
-/**
- * Determine if we're running on server or client
- */
-const isServer = typeof window === 'undefined';
-
-/**
- * Interface for the EnhancedQueryProvider props
- */
-interface EnhancedQueryProviderProps {
-  children: ReactNode;
-  /**
-   * Optional override for default stale time
-   */
-  defaultStaleTime?: number;
-  /**
-   * Optional override for default cache time (gcTime)
-   */
-  defaultGcTime?: number;
-  /**
-   * Whether to enable devtools in development
-   * @default true
-   */
-  enableDevtools?: boolean;
-}
-
-/**
- * Creates a QueryClient instance with optimized configuration
- */
-const createQueryClient = (networkStatus: { online: boolean }) => {
-  return new QueryClient({
-    defaultOptions: {
-      queries: {
-        // Network-aware refetch behavior
-        refetchOnWindowFocus: networkStatus.online,
-        refetchOnReconnect: networkStatus.online,
-        refetchOnMount: networkStatus.online,
-
-        // Optimize for offline usage
-        retry: networkStatus.online ? 2 : 0,
-
-        // Cache configuration
-        staleTime: isServer ? 10 * 60 * 1000 : 5 * 60 * 1000, // 10 mins server, 5 mins client
-        gcTime: 15 * 60 * 1000, // 15 minutes
-
-        // Suspense support
-        suspense: false,
-
-        // Show better error messages in development
-        useErrorBoundary: process.env.NODE_ENV === 'production',
-      },
-      mutations: {
-        // Network-aware retry behavior
-        retry: networkStatus.online ? 1 : 0,
-
-        // Better feedback during mutations
-        onMutate: () => {
-          if (!networkStatus.online) {
-            console.warn('Attempting mutation while offline. This may be queued if offline mutations are enabled.');
-          }
-        },
-
-        // Use error boundaries in production
-        useErrorBoundary: process.env.NODE_ENV === 'production',
-      },
-    },
-  });
-};
-
-/**
- * Network-aware QueryClient hook
- * Creates a QueryClient that responds to network status changes
- */
-const useNetworkAwareQueryClient = () => {
-  const networkStatus = useNetworkStatus();
-  const [queryClient, setQueryClient] = useState<QueryClient | null>(null);
-
-  // Create queryClient only after we have network status
-  useEffect(() => {
-    const client = createQueryClient(networkStatus);
-    setQueryClient(client);
-
-    // Listen for online/offline events to update QueryClient behavior
-    const handleOnline = () => {
-      console.log('Network connection detected. Resuming normal operation.');
-      // Could trigger refetches for stale queries
-      client.resumePausedMutations();
-    };
-
-    const handleOffline = () => {
-      console.log('Network connection lost. Pausing background operations.');
-      // Could pause background refetches
-    };
-
-    window.addEventListener('online', handleOnline);
-    window.addEventListener('offline', handleOffline);
-
-    return () => {
-      window.removeEventListener('online', handleOnline);
-      window.removeEventListener('offline', handleOffline);
-      client.clear();
-    };
-  }, [networkStatus]);
-
-  return queryClient;
-};
-
-/**
- * Enhanced Query Provider Component

-/**
- * Hook for fetching a list of products with filters
- */
-export const useProducts = (filters: ProductFilters = {}) => {
-  const { networkStatus, shouldUseFallback, markConnectionSuccess, markConnectionFailure } = 
-    useNetworkAwareService('product');
-  
-  const query = useQuery({
-    ...productsOptions(filters),
-    enabled: !shouldUseFallback(),
-    onSuccess: () => markConnectionSuccess(),
-    onError: (error) => {
-      logger.error('Error in useProducts hook', {
-        context: { feature: 'product', action: 'useProducts' },
-        data: error
-      });
-      markConnectionFailure(error);
-    }
-  });
-
-  // Use fallback data if needed
-  let productsData = query.data;
-  let isOffline = false;
-
-  if (shouldUseFallback() || (!query.data && query.isError)) {
-    logger.info('Using fallback data for products list', {
-      context: { feature: 'product', action: 'useProducts' }
-    });
-    isOffline = true;
-    
-    // Filter mock products based on filters
-    let filteredProducts = [...mockProducts];
-    
-    if (filters.category_id) {
-      filteredProducts = filteredProducts.filter(p => p.category_id === Number(filters.category_id));
-    }
-    
-    if (filters.producer_id) {
-      filteredProducts = filteredProducts.filter(p => p.producer_id === Number(filters.producer_id));
-    }
-    
-    if (filters.price_min !== undefined) {
-      filteredProducts = filteredProducts.filter(p => p.price >= Number(filters.price_min));
-    }
-    
-    if (filters.price_max !== undefined) {
-      filteredProducts = filteredProducts.filter(p => p.price <= Number(filters.price_max));
-    }
-    
-    if (filters.is_organic !== undefined) {
-      filteredProducts = filteredProducts.filter(p => p.is_organic === filters.is_organic);
-    }
-    
-    if (filters.is_featured !== undefined) {
-      filteredProducts = filteredProducts.filter(p => p.is_featured === filters.is_featured);
-    }
-    
-    if (filters.is_on_sale !== undefined) {
-      filteredProducts = filteredProducts.filter(p => p.is_on_sale === filters.is_on_sale);
-    }
-    
-    if (filters.search) {
-      const searchLowerCase = filters.search.toLowerCase();
-      filteredProducts = filteredProducts.filter(p => 
-        p.name.toLowerCase().includes(searchLowerCase) ||
-        (p.description && p.description.toLowerCase().includes(searchLowerCase)) ||
-        (p.short_description && p.short_description.toLowerCase().includes(searchLowerCase)) ||
-        (p.tags && p.tags.some(tag => tag.toLowerCase().includes(searchLowerCase)))
-      );
-    }
-    
-    // Create paginated response
-    const page = filters.page || 1;
-    const perPage = filters.per_page || 10;
-    const total = filteredProducts.length;
-    const lastPage = Math.ceil(total / perPage);
-    const offset = (page - 1) * perPage;
-    const paginatedProducts = filteredProducts.slice(offset, offset + perPage);
-    
-    productsData = {
-      data: paginatedProducts,
-      current_page: page,
-      last_page: lastPage,
-      per_page: perPage,
-      total: total
-    };
-  }
-  
-  return {
-    products: productsData?.data || [],
-    pagination: productsData ? {
-      currentPage: productsData.current_page,
-      lastPage: productsData.last_page,
-      perPage: productsData.per_page,
-      total: productsData.total
-    } : {
-      currentPage: 1,
-      lastPage: 1,
-      perPage: 10,
-      total: 0
-    },
-    isLoading: query.isLoading && !shouldUseFallback(),
-    isError: query.isError && !shouldUseFallback(),
-    error: query.error,
-    isOffline,
-    refetch: query.refetch,
-    ...query,
-  };
-};

-export function useAdminCategories() {
-  const options = adminCategoriesQueryOptions();
-  const query = useQuery(options);
-
-  const response = query.data as AdminCategoriesResponse | undefined;
-
-  // Organize categories into hierarchical structure
-  const organizeCategories = (categories: AdminCategory[]): AdminCategory[] => {
-    const categoryMap = new Map<number, AdminCategory>();
-    categories.forEach(category => {
-      categoryMap.set(category.id, { ...category, children: [] });
-    });
-
-    const rootCategories: AdminCategory[] = [];
-
-    categoryMap.forEach(category => {
-      if (category.parent_id === null) {
-        rootCategories.push(category);
-      } else {
-        const parent = categoryMap.get(category.parent_id);
-        if (parent) {
-          if (!parent.children) {
-            parent.children = [];
-          }
-          parent.children.push(category);
-        }
-      }
-    });
-
-    return rootCategories;
-  };
-
-  const organizedCategories = response?.data ? organizeCategories(response.data) : [];
-
-  return {
-    categories: organizedCategories,
-    allCategories: response?.data || [],
-    stats: response?.stats || null,
-    isLoading: query.status === 'pending',
-    isError: query.status === 'error',
-    error: query.error,
-    refetch: query.refetch,
-    isRefetching: query.isRefetching
-  };
-}
